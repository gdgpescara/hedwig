{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "skipLibCheck": true,
    "allowJs": true,
    "paths": {
      "~/components/*": [
        "./src/components/*"
      ],
      "~/layouts/*": [
        "./src/layouts/*"
      ],
      "~/constants": [
        "./src/constants"
      ],
    }
  }
}